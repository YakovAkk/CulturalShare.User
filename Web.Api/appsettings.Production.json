{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=Production_CulturalShare.User;Username=docker;Password=docker;"
  },
  "Serilog": {
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\CulturalShare\\UserService\\log-.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true
        }
      }
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Protocols": "Http2",
        "Url": "http://+:8080"
      },
      "webApi": {
        "Protocols": "Http1",
        "Url": "http://+:80"
      }
    }
  }
}
